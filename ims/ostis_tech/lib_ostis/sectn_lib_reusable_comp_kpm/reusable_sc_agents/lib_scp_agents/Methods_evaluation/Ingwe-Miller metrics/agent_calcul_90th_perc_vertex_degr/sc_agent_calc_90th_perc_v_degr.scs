sc_agent_of_calculation_ninetieth_percentile_vertex_degrees
=> nrel_main_idtf:
	[sc-агент подсчета 90-ой процентили степени вершины онтологии] (* <- lang_ru;; *);
	[sc-agent of calculation ninetieth percentile of vertex degrees] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_calculation_ninetieth_percentile_vertex_degrees_condition => ..sc_agent_of_calculation_ninetieth_percentile_vertex_degrees_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_to_calculate_ninetieth_percentile_vertex_degrees
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_calculation_ninetieth_percentile_vertex_degrees
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_calculation_ninetieth_percentile_vertex_degrees
		};;

	-> sc_agent_of_calculation_ninetieth_percentile_vertex_degrees_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_calculation_ninetieth_percentile_vertex_degrees_condition
= [*
	action_to_calculate_ninetieth_percentile_vertex_degrees _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_calculation_ninetieth_percentile_vertex_degrees_result
= [*
	action_to_calculate_ninetieth_percentile_vertex_degrees _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
