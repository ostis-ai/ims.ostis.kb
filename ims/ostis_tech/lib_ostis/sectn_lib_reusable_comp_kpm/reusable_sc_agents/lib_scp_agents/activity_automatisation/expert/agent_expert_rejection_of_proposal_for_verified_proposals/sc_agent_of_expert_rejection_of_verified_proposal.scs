sc_agent_of_expert_rejection_of_verified_proposal
=> nrel_main_idtf:
	[sc-агент отклонения верифицируемого предложения экспертом] (* <- lang_ru;; *);
	[sc-agent of expert rejection of verified proposal] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_sc_agent_action_class:
    action_of_expert_rejection_of_verified_proposal;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_expert_rejection_of_verified_proposal_condition => ..sc_agent_of_expert_rejection_of_verified_proposal_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_expert_rejection_of_verified_proposal
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_expert_rejection_of_verified_proposal
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_expert_rejection_of_verified_proposal	
		};;

	-> sc_agent_of_expert_rejection_of_verified_proposal_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_expert_rejection_of_verified_proposal_condition
= [*
	action_of_expert_rejection_of_verified_proposal _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_expert_rejection_of_verified_proposal_result
= [*
	action_of_expert_rejection_of_verified_proposal _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
