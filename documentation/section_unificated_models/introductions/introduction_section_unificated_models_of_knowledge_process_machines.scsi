				"file://introduction_section_unificated_models_of_knowledge_process_machines_htmls/explanation_for_sc_event.html"
				(*
				<- sc_machine_file;;
				<- lang_ru;;
				*);;
			*);;
		*);
	<= nrel_subdividing: 
	{
		sc_event_add_output_arc; /* calls after adding */
		sc_event_add_input_arc; /* calls after adding */
		sc_event_add_incident_edge; /* calls before removing */
		sc_event_remove_output_arc; /* calls before removing */
		sc_event_remove_input_arc; /* calls before removing */
		sc_event_remove_incident_edge; /* calls before removing */
		sc_event_sc_element_removing; /* calls before deleting */
		sc_event_change_link_content
	};;

